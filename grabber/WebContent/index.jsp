        });
        $('#SearchCondition1_txtKeyword').bind('blur', function () {
            $('#btnSearch').removeClass('highlight_submit_btn');
        });
        $('#SearchCondition1_txt_Price1').bind('blur', function () {
            $('#btnSearchRange').removeClass('highlight_submit_btn');
        });
        $('#SearchCondition1_txt_Price2').bind('blur', function () {
            $('#btnSearchRange').removeClass('highlight_submit_btn');
        });
    });
</script>
